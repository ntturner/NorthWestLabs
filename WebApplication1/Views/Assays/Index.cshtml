@model IEnumerable<WebApplication1.Models.Assay>

@{
    ViewBag.Title = "Assays";
}

<h2>Assays</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AssayName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Duration)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Protocol)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AssayName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Duration) days
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Protocol)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AssayID }) |
            @Html.ActionLink("Details", "Details", new { id=item.AssayID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AssayID })
        </td>
    </tr>
}

</table>
<p>
    @Html.ActionLink("Create New Assay", "Create")
</p>
<br />
<br />

<a href="@Url.Action("Index", "Singapore")" class="btn btn-lg btn-danger">Return Home</a>
